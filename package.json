{
  "name": "notion-like-button",
  "version": "1.0.1",
  "description": "chrome-extension-typescript-starter",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config webpack/webpack.dev.js --watch",
    "build:clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build:zip": "zip -r dist.zip dist",
    "build": "run-s build:*",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint --cache './**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check './**/*.{js,jsx,ts,tsx}'",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Yusuke Fujiki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fujikky/notion-like-button.git"
  },
  "dependencies": {
    "@linaria/core": "3.0.0-beta.15",
    "@linaria/react": "3.0.0-beta.15",
    "@notionhq/client": "1.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/shaker": "3.0.0-beta.15",
    "@linaria/webpack-loader": "3.0.0-beta.16",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/chrome": "0.0.186",
    "@types/jest": "27.5.1",
    "@types/mini-css-extract-plugin": "2.5.1",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "8.15.0",
    "eslint-config-timetree": "1.7.0",
    "mini-css-extract-plugin": "2.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2 ",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.4",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie > 0",
    "not op_mini all"
  ]
}
